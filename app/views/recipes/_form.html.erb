<%= form_for @recipe do |f| %>
    <%= f.label :title %><br>
    <%= f.text_field :title %><br>
    <%= f.fields_for :ingredients do |form_b| %>
        <br>Ingredient:<br>
        <%= form_b.label :name %><br>
        <%= form_b.text_field :name %><br>
        <%= form_b.label :quantity %><br>
        <%= form_b.number_field :quantity %><br>
    <% end %>
    <%= f.submit %>
<% end %>
